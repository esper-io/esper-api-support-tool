    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Main",
            "type": "python",
            "request": "launch",
            "program": "Main.py",
            "console": "integratedTerminal",
            "args": [
                "--record_place",
                "false",
                "--print_res",
                "false",
                "--print_duration",
                "false",
                "--log_api",
                "false",
                "--debug",
                "true"
            ]
        },
        {
            "name": "Create Executable",
            "type": "python",
            "request": "launch",
            "program": "generateExe.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}